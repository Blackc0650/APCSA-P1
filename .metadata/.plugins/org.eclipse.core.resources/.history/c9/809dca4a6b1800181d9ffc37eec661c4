package labs.unit7;
import java.util.ArrayList;
import java.util.Arrays;
import java.util.Collections;
public class Triples
{
   public static int number;

	public Triples()
	{
		this(0);
	}

	public Triples(int num)
	{
		number = num;
	}

	public void setNum(int num)
	{
		number = num;

	}
	
	public static int greatestCommonFactor(int a, int b, int c)
	{
		return eculids(eculids(a,b),c);
		
	}
	private static int eculids(int a,int b) {
		int result = 0;
		while(a % 2 == 0 && b % 2 == 0) {
			a /= 2; b /= 2; result++;
		}
		while(a != b) {
			if(a % 2 == 0) {
				a /= 2;
			}
			else if(b % 2 == 0) {
				b /= 2;
			}
			else if(a > b) {
				a = (a-b) / 2;
			}
			else {
				b = (b - a)/2;
			}
		}
		return (int)(a*Math.pow(2, result));
	}
	
	public String toString()
	{
		String output="";






		return output+"\n";
	}
}