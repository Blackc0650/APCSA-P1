	private ArrayList<Integer> preformWhileArray(int i) {
		int count = i  / 2;
		ArrayList<Integer> factors = new ArrayList<Integer>();
		while(count > 0) {
			if(((double)(i / count)) % 1 == 0) {
				factors.add(count);
			}
			count--;
		}
		return Arrays.sort(factors);
	}

